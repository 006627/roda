<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:i="urn:import:config.i18n.client" xmlns:d="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:u="urn:import:org.roda.wui.management.user.client">

	<ui:with field='constants' type='config.i18n.client.UserManagementConstants' />

	<ui:style>
	
	</ui:style>

	<g:FlowPanel styleName="wui-management-user" addStyleNames="wrapper skip_padding">
		<g:FlowPanel addStyleNames="row full_width skip_padding">
			<g:FlowPanel addStyleNames="col_10 content">
				<g:Label styleName="h1">Log Entry</g:Label>
				<g:Label styleName="descriptiveMetadata-field-key">Identifier</g:Label>
				<g:Label ui:field="logId" />
				<g:Label styleName="descriptiveMetadata-field-key">Component</g:Label>
				<g:Label ui:field="logComponent" />
				<g:Label styleName="descriptiveMetadata-field-key">Method</g:Label>
				<g:Label ui:field="logMethod" />
				<g:Label styleName="descriptiveMetadata-field-key">Address</g:Label>
				<g:Label ui:field="logAddress" />
				<g:Label styleName="descriptiveMetadata-field-key">Datetime</g:Label>
				<g:Label ui:field="logDatetime" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="logRelatedObjectLabel">Related
					object</g:Label>
				<g:Label ui:field="logRelatedObject" />
				<g:Label styleName="descriptiveMetadata-field-key">Username</g:Label>
				<g:Label ui:field="logUsername" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="logParametersLabel">Parameters</g:Label>
				<g:FlowPanel ui:field="logParameters" />
			</g:FlowPanel>

			<g:FlowPanel addStyleNames="col_2 last sidebar">
				<g:FlowPanel>
					<g:Label addStyleNames="h4">
						<ui:msg>Actions</ui:msg>
					</g:Label>
					<g:Button addStyleNames="btn btn-times-circle" ui:field="buttonCancel">
						<ui:text from='{constants.editUserCancel}' />
					</g:Button>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
