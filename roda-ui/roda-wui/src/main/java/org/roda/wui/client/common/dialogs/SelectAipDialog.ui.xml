<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:i="urn:import:config.i18n.client" xmlns:m="urn:import:org.roda.wui.client.main"
	xmlns:wcag="urn:import:org.roda.wui.common.client.widgets.wcag"
	xmlns:common="urn:import:org.roda.wui.client.common" xmlns:lists="urn:import:org.roda.wui.client.common.lists">

	<ui:style>
	.searchlist-title {
		position: absolute;
	}
	</ui:style>

	<ui:with field='messages' type='config.i18n.client.BrowseMessages' />

	<g:FlowPanel addStyleNames="selectAip wui-dialog-confirm wrapper skip_padding">
		<g:FlowPanel addStyleNames="wui-dialog-layout">
			<common:SearchPanel ui:field="searchPanel" />
			<g:ScrollPanel addStyleNames="selectAipResultsPanel">
				<lists:AIPList ui:field="searchResultsPanel" />
			</g:ScrollPanel>
			<g:Button addStyleNames="btn btn-ban" ui:field="cancelButton">
				<ui:text from='{messages.selectAipCancelButton}' />
			</g:Button>
			<g:Button addStyleNames="pull-right btn btn-play" ui:field="selectButton">
				<ui:text from='{messages.selectAipSelectButton}' />
			</g:Button>
			<g:Button addStyleNames="pull-right btn btn-play" ui:field="emptyParentButton">
				<ui:text from='{messages.selectAipEmptyParentButton}' />
			</g:Button>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
