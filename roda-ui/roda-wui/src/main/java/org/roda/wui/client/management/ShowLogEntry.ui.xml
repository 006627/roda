<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field='messages' type='config.i18n.client.ClientMessages' />

	<g:FlowPanel styleName="wui-management-user" addStyleNames="wrapper skip_padding">
		<g:FlowPanel addStyleNames="row full_width skip_padding">
			<g:FlowPanel addStyleNames="col_10 content">
				<g:FlowPanel styleName="wui-user-data" addStyleNames="wrapper skip_padding">
					<g:FlowPanel addStyleNames="row full_width no_padding">
						<g:FlowPanel addStyleNames="wui-data-panel">
							<g:Label styleName="h1">
								<ui:text from='{messages.showLogEntryTitle}' />
							</g:Label>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="logIdLabel">
									<ui:text from='{messages.logEntryIdentifier}' />
								</g:Label>
								<g:Label styleName="value" ui:field="logIdValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="logComponentLabel">
									<ui:text from='{messages.logEntryComponent}' />
								</g:Label>
								<g:Label styleName="value" ui:field="logComponentValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="logMethodLabel">
									<ui:text from='{messages.logEntryMethod}' />
								</g:Label>
								<g:Label styleName="value" ui:field="logMethodValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="logAddressLabel">
									<ui:text from='{messages.logEntryAddress}' />
								</g:Label>
								<g:Label styleName="value" ui:field="logAddressValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="logDatetimeLabel">
									<ui:text from='{messages.logEntryDatetime}' />
								</g:Label>
								<g:Label styleName="value" ui:field="logDatetimeValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="logRelatedObjectLabel">
									<ui:text from='{messages.logEntryRelatedObject}' />
								</g:Label>
								<g:Label styleName="value" ui:field="logRelatedObjectValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="logUsernameLabel">
									<ui:text from='{messages.logEntryUsername}' />
								</g:Label>
								<g:Label styleName="value" ui:field="logUsernameValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="logParametersLabel">
									<ui:text from='{messages.logEntryParameters}' />
								</g:Label>
								<g:FlowPanel styleName="value" ui:field="logParametersValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="logStateLabel">
									<ui:text from='{messages.logEntryState}' />
								</g:Label>
								<g:HTML styleName="value" ui:field="logStateValue" />
							</g:FlowPanel>
						</g:FlowPanel>
					</g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>

			<g:FlowPanel addStyleNames="col_2 last sidebar">
				<g:FlowPanel addStyleNames="sticky-flow">
					<g:FlowPanel addStyleNames="sticky-padding">
						<g:Label addStyleNames="h4">
							<ui:text from='{messages.sidebarActionsTitle}' />
						</g:Label>
						<g:Button addStyleNames="btn btn-block btn-default btn-times-circle"
							ui:field="buttonCancel">
							<ui:text from='{messages.backButton}' />
						</g:Button>
					</g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
