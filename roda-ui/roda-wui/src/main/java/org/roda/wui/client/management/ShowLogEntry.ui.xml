<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:i="urn:import:config.i18n.client" xmlns:d="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:u="urn:import:org.roda.wui.management.user.client">

	<ui:with field='messages' type='config.i18n.client.BrowseMessages' />

	<ui:style>

	</ui:style>

	<g:FlowPanel styleName="wui-management-user" addStyleNames="wrapper skip_padding">
		<g:FlowPanel addStyleNames="row full_width skip_padding">
			<g:FlowPanel addStyleNames="col_10 content">
				<g:FlowPanel styleName="wui-user-data" addStyleNames="wrapper skip_padding">
					<g:FlowPanel addStyleNames="row full_width no_padding">
						<g:FlowPanel addStyleNames="wui-data-panel">
							<g:Label styleName="h1">
								<ui:text from='{messages.showLogEntryTitle}' />
							</g:Label>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label"><ui:text from='{messages.logEntryIdentifier}' /></g:Label>
								<g:Label styleName="value" ui:field="logId" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label"><ui:text from='{messages.logEntryComponent}' /></g:Label>
								<g:Label styleName="value" ui:field="logComponent" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label"><ui:text from='{messages.logEntryMethod}' /></g:Label>
								<g:Label styleName="value" ui:field="logMethod" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label"><ui:text from='{messages.logEntryAddress}' /></g:Label>
								<g:Label styleName="value" ui:field="logAddress" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label"><ui:text from='{messages.logEntryDatetime}' /></g:Label>
								<g:Label styleName="value" ui:field="logDatetime" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="logRelatedObjectLabel">
									<ui:text from='{messages.logEntryRelatedObject}' />
								</g:Label>
								<g:Label styleName="value" ui:field="logRelatedObject" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label"><ui:text from='{messages.logEntryUsername}' /></g:Label>
								<g:Label styleName="value" ui:field="logUsername" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="logParametersLabel">
									<ui:text from='{messages.logEntryParameters}' />
								</g:Label>
								<g:FlowPanel styleName="value" ui:field="logParameters" />
							</g:FlowPanel>
						</g:FlowPanel>
					</g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>

			<g:FlowPanel addStyleNames="col_2 last sidebar">
				<g:FlowPanel>
					<g:Label addStyleNames="h4">
						<ui:text from='{messages.sidebarActionsTitle}' />
					</g:Label>
					<g:Button addStyleNames="btn btn-times-circle" ui:field="buttonCancel">
						<ui:text from='{messages.cancelButton}' />
					</g:Button>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
