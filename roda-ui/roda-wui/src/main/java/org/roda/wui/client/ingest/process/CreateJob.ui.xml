<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:i="urn:import:config.i18n.client" xmlns:d="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:u="urn:import:org.roda.wui.management.user.client">

	<ui:style>
	
	</ui:style>

	<g:FlowPanel styleName="wui-ingest-new" addStyleNames="wrapper skip_padding">
		<g:FlowPanel addStyleNames="row full_width skip_padding">
			<g:FlowPanel addStyleNames="col_10 content">
				<g:Label styleName="h1">New ingest process</g:Label>
				<g:Label addStyleNames="form-label">
					Name
				</g:Label>
				<g:TextBox ui:field="name" addStyleNames="form-textbox" />
				<g:Label addStyleNames="form-label">
					Selected submission information
					packages (SIP)
				</g:Label>
				<g:HTML addStyleNames="wui-transferredResourceList" ui:field="objectList" />
				<g:Label addStyleNames="form-label">
					Ingest workflow
				</g:Label>
				<g:ListBox addStyleNames="form-listbox" ui:field="ingestWorkflow" />
				<g:FlowPanel ui:field="ingestWorkflowOptions" />
			</g:FlowPanel>

			<g:FlowPanel addStyleNames="col_2 last sidebar">
				<g:FlowPanel>
					<g:Label addStyleNames="h4">
						<ui:msg>Actions</ui:msg>
					</g:Label>
					<g:Button addStyleNames="btn btn-play" ui:field="buttonCreate">
						Create
					</g:Button>
					<g:Button addStyleNames="btn btn-times-circle" ui:field="buttonCancel">
						Cancel
					</g:Button>

				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
