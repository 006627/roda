<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:i="urn:import:config.i18n.client" xmlns:d="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:u="urn:import:org.roda.wui.management.user.client" xmlns:p="urn:import:org.roda.wui.client.ingest.process"
	xmlns:lists="urn:import:org.roda.wui.client.common.lists">

	<ui:style>
	.reportTitle {
		margin-bottom: -25px;
		margin-top: 20px;
	}
	</ui:style>

	<g:FlowPanel styleName="wui-ingest-new" addStyleNames="wrapper skip_padding">
		<g:FlowPanel addStyleNames="row full_width skip_padding">
			<g:FlowPanel addStyleNames="col_10 content">
				<g:Label styleName="h1">Information package processing report</g:Label>
				<g:Label addStyleNames="label">
					id
				</g:Label>
				<g:Label ui:field="id" />
				<g:Label addStyleNames="label">
					Job
				</g:Label>
				<g:Label ui:field="job" />
				<g:Label addStyleNames="label">
					Submission Information Package
				</g:Label>
				<g:Label ui:field="objectId" />
				<g:Label addStyleNames="label">
					Archival Information Package
				</g:Label>
				<g:Label ui:field="aip" />

				<g:Label addStyleNames="label">
					Date created
				</g:Label>
				<g:Label ui:field="dateCreated" />
				<g:Label addStyleNames="label">
					Date updated
				</g:Label>
				<g:Label ui:field="dateUpdated" />
				<g:Label addStyleNames="h4">
					<ui:msg>Run tasks</ui:msg>
				</g:Label>
				<g:FlowPanel ui:field="reportAttributes" />
				<g:FlowPanel ui:field="reportItems" />

			</g:FlowPanel>

			<g:FlowPanel addStyleNames="col_2 last sidebar">
				<g:FlowPanel>
					<g:Label addStyleNames="h4">
						<ui:msg>Actions</ui:msg>
					</g:Label>
					<g:Button addStyleNames="btn btn-times-circle" ui:field="buttonBack">
						Back
					</g:Button>

				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
