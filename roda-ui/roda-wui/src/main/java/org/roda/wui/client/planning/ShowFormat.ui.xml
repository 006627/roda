<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:i="urn:import:config.i18n.client" xmlns:d="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:u="urn:import:org.roda.wui.management.user.client">

	<ui:with field='constants' type='config.i18n.client.UserManagementConstants' />

	<ui:style>

	</ui:style>

	<ui:with field='messages' type='config.i18n.client.BrowseMessages' />

	<g:FlowPanel styleName="wui-management-user" addStyleNames="wrapper skip_padding">
		<g:FlowPanel addStyleNames="row full_width skip_padding">
			<g:FlowPanel addStyleNames="col_10 content">
				<g:Label styleName="h1">Format</g:Label>
				<g:Label styleName="descriptiveMetadata-field-key">Identifier</g:Label>
				<g:Label ui:field="formatId" />
				<g:Label styleName="descriptiveMetadata-field-key">Name</g:Label>
				<g:Label ui:field="formatName" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="formatDefinitionKey">Definition</g:Label>
				<g:Label ui:field="formatDefinitionValue" />
				<g:Label styleName="descriptiveMetadata-field-key">Category</g:Label>
				<g:Label ui:field="formatCategory" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="formatLatestVersionKey">Latest Version</g:Label>
				<g:Label ui:field="formatLatestVersionValue" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="formatPopularityKey">Popularity</g:Label>
				<g:Label ui:field="formatPopularityValue" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="formatDeveloperKey">Developer</g:Label>
				<g:Label ui:field="formatDeveloperValue" />
				<g:Label styleName="descriptiveMetadata-field-key">Initial Release</g:Label>
				<g:Label ui:field="formatInitialRelease" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="formatStandardKey">Standard</g:Label>
				<g:Label ui:field="formatStandardValue" />
				<g:Label styleName="descriptiveMetadata-field-key">Open Format</g:Label>
				<g:Label ui:field="formatIsOpenFormat" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="formatWebsiteKey">Website</g:Label>
				<g:Label ui:field="formatWebsiteValue" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="formatProvenanceInformationKey">Provenance Information</g:Label>
				<g:Label ui:field="formatProvenanceInformationValue" />
				
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="formatExtensionsKey">Extensions</g:Label>
				<g:FlowPanel ui:field="formatExtensionsValue" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="formatMimetypesKey">Mimetypes</g:Label>
				<g:FlowPanel ui:field="formatMimetypesValue" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="formatPronomsKey">Pronoms</g:Label>
				<g:FlowPanel ui:field="formatPronomsValue" />
				<g:Label styleName="descriptiveMetadata-field-key" ui:field="formatUtisKey">UTIs</g:Label>
				<g:FlowPanel ui:field="formatUtisValue" />
			</g:FlowPanel>

			<g:FlowPanel addStyleNames="col_2 last sidebar">
				<g:FlowPanel>
					<g:Label addStyleNames="h4">
						<ui:text from='{messages.sidebarActionsTitle}' />
					</g:Label>
					<g:Button addStyleNames="btn btn-edit" ui:field="buttonEdit">
						<ui:text from='{messages.formatRegisterEditButton}' />
					</g:Button>
					<g:Button addStyleNames="btn btn-times-circle" ui:field="buttonCancel">
						<ui:text from='{constants.editUserCancel}' />
					</g:Button>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
