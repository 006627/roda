<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:i="urn:import:config.i18n.client" xmlns:d="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:u="urn:import:org.roda.wui.management.user.client">

	<ui:with field='constants' type='config.i18n.client.UserManagementConstants' />

	<ui:style>

	</ui:style>

	<g:FlowPanel styleName="wui-management-user" addStyleNames="wrapper skip_padding">
		<g:FlowPanel addStyleNames="row full_width skip_padding">
			<g:FlowPanel addStyleNames="col_10 content">
				<g:Label styleName="h1">Risk</g:Label>
				<g:Label styleName="descriptiveMetadata-field-key">Identifier</g:Label>
				<g:Label ui:field="agentId" />
				<g:Label styleName="descriptiveMetadata-field-key">Name</g:Label>
				<g:Label ui:field="agentName" />
				<g:Label styleName="descriptiveMetadata-field-key">Type</g:Label>
				<g:Label ui:field="agentType" />
				<g:Label styleName="descriptiveMetadata-field-key">Description</g:Label>
				<g:Label ui:field="agentDescription" />
				<g:Label styleName="descriptiveMetadata-field-key">Category</g:Label>
				<g:Label ui:field="agentCategory" />
				<g:Label styleName="descriptiveMetadata-field-key">Version</g:Label>
				<g:Label ui:field="agentVersion" />
				<g:Label styleName="descriptiveMetadata-field-key">License</g:Label>
				<g:Label ui:field="agentLicense" />		
				<g:Label styleName="descriptiveMetadata-field-key">Popularity</g:Label>
				<g:Label ui:field="agentPopularity" />
				<g:Label styleName="descriptiveMetadata-field-key">Initial Release</g:Label>
				<g:Label ui:field="agentInitialRelease" />
				<g:Label styleName="descriptiveMetadata-field-key">Website</g:Label>
				<g:Label ui:field="agentWebsite" />
				<g:Label styleName="descriptiveMetadata-field-key">Download</g:Label>
				<g:Label ui:field="agentDownload" />		
				<g:Label styleName="descriptiveMetadata-field-key">Provenance Information</g:Label>
				<g:Label ui:field="agentProvenanceInformation" />
			</g:FlowPanel>

			<g:FlowPanel addStyleNames="col_2 last sidebar">
				<g:FlowPanel>
					<g:Label addStyleNames="h4">
						<ui:msg>Actions</ui:msg>
					</g:Label>
					<g:Button addStyleNames="btn btn-times-circle" ui:field="buttonEdit">
						Edit
					</g:Button>
					<g:Button addStyleNames="btn btn-times-circle" ui:field="buttonCancel">
						Cancel
					</g:Button>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
