<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:i="urn:import:config.i18n.client" xmlns:d="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:lists="urn:import:org.roda.wui.client.common.lists">

	<ui:with field='messages' type='config.i18n.client.ClientMessages' />

	<ui:style>
	
	</ui:style>

	<g:FlowPanel styleName="wui-management-user" addStyleNames="wrapper skip_padding">
		<g:FlowPanel addStyleNames="row full_width skip_padding">
			<g:FlowPanel addStyleNames="col_10 content">
				<g:FlowPanel styleName="wui-user-data" addStyleNames="wrapper skip_padding">
					<g:FlowPanel addStyleNames="row full_width no_padding">
						<g:FlowPanel addStyleNames="wui-data-panel">

							<g:Label styleName="h1">
								<ui:text from='{messages.showAgentTitle}' />
							</g:Label>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label">Identifier</g:Label>
								<g:Label styleName="value" ui:field="agentId" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label">
									<ui:text from='{messages.agentName}' />
								</g:Label>
								<g:Label styleName="value" ui:field="agentName" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="agentTypeKey">
									<ui:text from='{messages.agentType}' />
								</g:Label>
								<g:Label styleName="value" ui:field="agentTypeValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="agentDescriptionKey">
									<ui:text from='{messages.agentDescription}' />
								</g:Label>
								<g:Label styleName="value" ui:field="agentDescriptionValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label">
									<ui:text from='{messages.agentCategory}' />
								</g:Label>
								<g:Label styleName="value" ui:field="agentCategory" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="agentVersionKey">
									<ui:text from='{messages.agentVersion}' />
								</g:Label>
								<g:Label styleName="value" ui:field="agentVersionValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="agentLicenseKey">
									<ui:text from='{messages.agentLicense}' />
								</g:Label>
								<g:Label styleName="value" ui:field="agentLicenseValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="agentPopularityKey">
									<ui:text from='{messages.agentPopularity}' />
								</g:Label>
								<g:Label styleName="value" ui:field="agentPopularityValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label">
									<ui:text from='{messages.agentInitialRelease}' />
								</g:Label>
								<g:Label styleName="value" ui:field="agentInitialRelease" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="agentWebsiteKey">
									<ui:text from='{messages.agentWebsite}' />
								</g:Label>
								<g:Label styleName="value" ui:field="agentWebsiteValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="agentDownloadKey">
									<ui:text from='{messages.agentDownload}' />
								</g:Label>
								<g:Label styleName="value" ui:field="agentDownloadValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="agentProvenanceInformationKey">
									<ui:text from='{messages.agentProvenanceInformation}' />
								</g:Label>
								<g:Label styleName="value" ui:field="agentProvenanceInformationValue" />
							</g:FlowPanel>

							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="platformsKey">
									<ui:text from='{messages.agentPlatforms}' />
								</g:Label>
								<g:FlowPanel styleName="value" ui:field="platformsValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="extensionsKey">
									<ui:text from='{messages.agentExtensions}' />
								</g:Label>
								<g:FlowPanel styleName="value" ui:field="extensionsValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="mimetypesKey">
									<ui:text from='{messages.agentMimetypes}' />
								</g:Label>
								<g:FlowPanel styleName="value" ui:field="mimetypesValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="pronomsKey">
									<ui:text from='{messages.agentPronoms}' />
								</g:Label>
								<g:FlowPanel styleName="value" ui:field="pronomsValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="utisKey">
									<ui:text from='{messages.agentUtis}' />
								</g:Label>
								<g:FlowPanel styleName="value" ui:field="utisValue" />
							</g:FlowPanel>

							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="formatIdsKey">
									<ui:text from='{messages.agentFormats}' />
								</g:Label>
								<g:FlowPanel styleName="value" ui:field="formatIdsValue" />
							</g:FlowPanel>
							<g:FlowPanel styleName="field">
								<g:Label styleName="label" ui:field="requiredAgentsKey">
									<ui:text from='{messages.agentRequiredAgents}' />
								</g:Label>
								<g:FlowPanel styleName="value" ui:field="requiredAgentsValue" />
							</g:FlowPanel>

						</g:FlowPanel>
					</g:FlowPanel>
				</g:FlowPanel>

			</g:FlowPanel>

			<g:FlowPanel addStyleNames="col_2 last sidebar">
				<g:FlowPanel>
					<g:Label addStyleNames="h4">
						<ui:text from='{messages.sidebarActionsTitle}' />
					</g:Label>
					<g:Button addStyleNames="btn btn-edit" ui:field="buttonEdit">
						<ui:text from='{messages.editButton}' />
					</g:Button>
					<g:Button addStyleNames="btn btn-times-circle" ui:field="buttonCancel">
						<ui:text from='{messages.cancelButton}' />
					</g:Button>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
